ubuntu.packages = libltdl7
git.url = 'https://git.aydo.com/binary/cfssl.git'

git.export.1                   =
    dest:'$(system.paths.bin)/cfssl',
    link:'True',
    platform:'ubuntu64',
    source:'cfssl',
    url:'$(git.url)',

git.export.2                   =
    dest:'$(system.paths.bin)/cfssljson',
    link:'True',
    platform:'ubuntu64',
    source:'cfssljson',
    url:'$(git.url)',

platform.supported             =
    'linux64',
