param.base                     = '/opt/influxdb/'

platform.supported             =
    'linux64',
    'darwin',

process.1                      =
    args:,
    cmd:'./influxd -config=cfg/config.toml',
    cwd:'/opt/influxdb/',
    filterstr:'./influxd -config=cfg/config.toml',
    name:'$(name)__$(instance)',
    platform:'ubuntu',
    ports:'8086;8083',
    prio:'5',
    startupmanager:'tmux',
    timeout_start:'10',
    timeout_stop:'10',

